{
    "name": "catbug-bot",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "tsc",
        "format": "run-s format-package-json format-imports format-prettier format-eslint",
        "format-eslint": "eslint --cache --ext .ts --fix ./",
        "format-imports": "import-sort --write src/**/*.ts",
        "format-package-json": "sort-package-json",
        "format-prettier": "prettier --write src/**/*.ts",
        "lint": "run-p lint-eslint lint-markdown",
        "lint-eslint": "eslint --cache --ext .ts --format=pretty ./",
        "lint-markdown": "markdownlint **/*.md .**/**/*.md --ignore build --ignore coverage --ignore dist --ignore node_modules",
        "start": "npm run build && node build/run.js",
        "start-dev": "env NODE_ENV=dev npm run start",
        "test": "jest --passWithNoTests",
        "test--coverage": "npm run test -- --coverage",
        "test--watch": "npm run test -- --watch"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "dependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.9.0",
        "@types/fuzzyset.js": "0.0.2",
        "@types/jest": "^25.2.1",
        "@types/mongoose": "^5.7.10",
        "@types/node": "^13.11.1",
        "@types/ws": "^7.2.4",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.28.0",
        "chalk": "^4.0.0",
        "date-fns": "^2.12.0",
        "discord.js": "^12.1.1",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-airbnb-typescript": "^8.0.2",
        "eslint-config-prettier": "^6.11.0",
        "eslint-formatter-pretty": "^4.0.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-jest": "^23.13.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-unicorn": "^20.1.0",
        "fuzzyset.js": "0.0.91",
        "husky": "^4.2.5",
        "import-sort-cli": "^6.0.0",
        "import-sort-parser-typescript": "^6.0.0",
        "import-sort-style-module": "^6.0.0",
        "jest": "^25.3.0",
        "lint-staged": "^10.2.11",
        "markdownlint-cli": "^0.23.1",
        "markov-typescript": "^1.1.0",
        "mongodb": "^3.5.9",
        "mongodb-memory-server": "^6.5.2",
        "mongoose": "^5.9.9",
        "npm-run-all": "^4.1.5",
        "prettier": "2.0.4",
        "sort-package-json": "^1.44.0",
        "ts-jest": "^25.4.0",
        "ts-mongoose": "0.0.21",
        "ts-node": "^8.8.2",
        "typescript": "^3.8.3"
    }
}
